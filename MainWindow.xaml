<Window x:Class="SzambevitelWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="400">
    <Grid x:Name="foGrid">

        <Grid.RowDefinitions>
            <RowDefinition Height="71*"/>
            <RowDefinition Height="413*"/>
        </Grid.RowDefinitions>

        <Button x:Name="btnSzam" Content="Generál" HorizontalAlignment="Left" Margin="10,49,0,0" VerticalAlignment="Top" Width="120" Click="Generalas"/>
        <RadioButton x:Name="rdbtJelolt" Content="Jelölt" HorizontalAlignment="Left" Margin="165,24,0,0" VerticalAlignment="Top"/>
        <RadioButton x:Name="rdbtJeloletlen" Content="Jelöletlen" HorizontalAlignment="Left" Margin="165,49,0,0" VerticalAlignment="Top"/>

        <StackPanel x:Name="stckpnlKontener" Grid.Row="1" HorizontalAlignment="Left" Height="Auto" Margin="10,10,0,0" VerticalAlignment="Top" Width="Auto"/>

    </Grid>
</Window>
